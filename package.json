{
	"name": "thello",
	"displayName": "thello",
	"description": "golang implements interface quickly",
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/kosmgco/thello"
	},
	"engines": {
		"vscode": "^1.65.0"
	},
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
		"onLanguage:go",
		"onCommand:thello.helloWorld"
	],
	"publisher": "ooops",
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "thello.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "code-actions-sample.command",
				"title": "ddd"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.65.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.12.1",
		"@typescript-eslint/parser": "^5.12.1",
		"eslint": "^8.9.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.1",
		"typescript": "^4.5.5",
		"@vscode/test-electron": "^2.1.2"
	}
}
